@model IEnumerable<PhotowalaWebsite.Models.Image>

@{
    ViewBag.Title = "webTeamPhoto";
    Layout = null;
}

@foreach (var items in Model)
{
    <div class="col-md-6 col-lg-3 ftco-animate">
        <div class="staff">
            <img src="@items.ImagePath" style="width:200px; height:200px;" class="figure-img img-fluid" />
            <div class="text pt-4">
                <h3><a>@items.TeamName</a></h3>
                <span class="position mb-2">@items.Position</span>
                <p>@items.Description</p>

            </div>
        </div>
    </div>
}

