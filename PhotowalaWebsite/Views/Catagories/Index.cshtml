@model IEnumerable<PhotowalaWebsite.Models.Catagory>

@{
    ViewBag.Title = "Index";
}


<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CatagoryName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CatagoryName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CatagoryId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CatagoryId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CatagoryId })
        </td>
    </tr>
}

</table>
